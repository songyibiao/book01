{
    "title" : "给新手的两轮自平衡小车制作指南",
	"author": "宋一标",
    "description": "面向新手的两轮自平衡小车设计、制作教程。",
	"language" : "zh-hans",
	"styles": {
    "website": "styles/website.css"
    },
	"links" : {
		"sidebar" : {
			"给新手的两轮自平衡小车制作指南" : "https://c.miaowlabs.com/"
		}
    },
	"plugins": [
		"-search",
		"-highlight",
		"splitter",
		"back-to-top-button",
		"copy-code-button",
		"prism",
		"include-codeblock",
		"ace",
		"katex",
		"image-captions",
		"popup",
		"fontsettings",
		"theme-comscore"
    ],
    "pluginsConfig": {
		"theme-default": {
			"showLevel": true
		},
        "prism": {
            "css": [
                "prismjs/themes/prism-tomorrow.css"				
            ]			
        },	
		"include-codeblock": {
            "template": "ace",
            "unindent": true,
            "theme": "monokai",
			"edit": true
        },
		"fontsettings": {
            "theme": "white", 
            "family": "sans", 
            "size": 2        
		},
		"image-captions": {
			"caption": "Image _PAGE_LEVEL_._PAGE_IMAGE_NUMBER_ - _CAPTION_"
		}
    }
}


